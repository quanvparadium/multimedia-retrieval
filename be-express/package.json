{
    "name": "expressjs",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "tsc && tsc-alias",
        "start": "node dist/main.js",
        "dev": "nodemon -e ts,js --exec ts-node -r tsconfig-paths/register src/main.ts",
        "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
        "typeorm": "typeorm-ts-node-commonjs",
        "typeorm:run": "npm run typeorm migration:run -- -d ./typeOrm.config.ts",
        "typeorm:generate": "npm run typeorm -- -d ./typeOrm.config.ts migration:generate ./migrations/%npm_config_name%",
        "typeorm:create": "npm run typeorm -- migration:create ./migrations/$npm_config_name",
        "typeorm:revert": "npm run typeorm -- -d ./typeOrm.config.ts migration:revert",
        "argument": "echo \"The value of --foo is %npm_config_foo%\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "base-64": "^1.0.0",
        "bcrypt": "^5.1.1",
        "class-validator": "^0.14.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "formidable": "^3.5.1",
        "jsonwebtoken": "^9.0.2",
        "mongodb": "^5.9.2",
        "morgan": "^1.10.0",
        "pg": "^8.11.5",
        "reflect-metadata": "^0.2.2",
        "sharp": "^0.33.3",
        "typeorm": "^0.3.20"
    },
    "devDependencies": {
        "@types/base-64": "^1.0.2",
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/formidable": "^3.4.5",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.12.7",
        "prettier": "^3.2.5",
        "ts-alias": "^0.0.7",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.4.2"
    }
}
